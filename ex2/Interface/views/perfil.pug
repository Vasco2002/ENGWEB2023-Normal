extends layout

block content
  .w3-card-4
    header.w3-indigo
      h1 ID da Planta:  #{list._id}

    .w3-container
      table.w3-table-all
        p Número de Registo: #{list.numeroRegisto}
        p Código de rua: #{list.codigoRua}
        p Rua: #{list.rua}
        p Local: #{list.local}
        p Freguesia: #{list.freguesia}
        p Espécie: #{list.especie}
        p Nome Científico: #{list.nomeCientifico}
        p Origem: #{list.origem}
        p Data de Plantação: #{list.dataPlantacao}
        p Estado: #{list.estado}
        p Caldeira: #{list.caldeira}
        p Tutor: #{list.tutor}
        p Implantação: #{list.implantacao}
        p Gestor: #{list.gestor}
        p Data de Atualização: #{list.dataAtualizacao}
        p Número de Intervenções: #{list.numeroIntervencoes}

  footer.w3-container.w3-indigo 
    address Generated by plantas::EngWeb2023 em #{d}
  